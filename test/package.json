{
  "name": "enzyme-test-suite",
  "private": true,
  "version": "1.0.0",
  "description": "JavaScript Testing utilities for React",
  "homepage": "https://enzymejs.github.io/enzyme/",
  "scripts": {
    "build": "babel --source-maps=both test -d build",
    "clean": "rimraf build",
    "lint": "eslint . --ext .jsx,.js",
    "mocha": "yarn build && mocha --recursive build",
    "test": "yarn lint"
  },
  "keywords": [
    "javascript",
    "shallow rendering",
    "shallowRender",
    "test",
    "reactjs",
    "react",
    "flux",
    "testing",
    "test utils",
    "assertion helpers",
    "tdd",
    "mocha"
  ],
  "author": "Jordan Harband <ljharb@gmail.com>",
  "contributors": [
    "Wojciech Maj <kontakt@wojtekmaj.pl>",
    "Nicolas Gilbert <nic.g.gilbert@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@chalbert/enzyme-adapter-react-18": "portal:../",
    "@chalbert/enzyme-adapter-utils": "^0.1.0",
    "chai": "^4.3.6",
    "create-react-class": "^15.7.0",
    "enzyme": "^3.11.0",
    "enzyme-shallow-equal": "^1.0.4",
    "has-symbols": "^1.0.3",
    "html-element-map": "^1.3.1",
    "jsdom": "^19.0.0",
    "lodash.isequal": "^4.5.0",
    "mocha": "^9.2.2",
    "mocha-wrap": "^2.1.2",
    "object-inspect": "^1.12.2",
    "prop-types": "^15.8.1",
    "raf": "^3.4.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-is": "^18.1.0",
    "semver": "^7.3.7",
    "sinon": "^14.0.0",
    "sinon-sandbox": "^2.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "eslint": "^8.16.0",
    "eslint-config-wojtekmaj": "^0.6.6",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chalbert/enzyme-adapter-react-18.git",
